#!/etc/network/interface
auto lo                           
iface lo inet loopback            
                                  
auto enp0s3                       
iface enp0s3 inet static          
        address 10.10.10.2        
        netmask 255.255.0.0       
        gateway 10.10.10.1        
        dns-nameservers 10.10.10.1
